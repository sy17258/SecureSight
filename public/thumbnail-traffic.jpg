<svg width="640" height="480" xmlns="http://www.w3.org/2000/svg">
  <!-- Background gradient -->
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1a1a2e;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#16213e;stop-opacity:1" />
    </linearGradient>
    <filter id="greenGlow">
      <feGaussianBlur stdDeviation="3" result="coloredBlur"/>
      <feMerge> 
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  
  <rect width="640" height="480" fill="url(#bg)"/>
  
  <!-- Grid pattern -->
  <defs>
    <pattern id="grid" width="40" height="40" patternUnits="userSpaceOnUse">
      <path d="M 40 0 L 0 0 0 40" fill="none" stroke="#2a3441" stroke-width="1" opacity="0.3"/>
    </pattern>
  </defs>
  <rect width="640" height="480" fill="url(#grid)"/>
  
  <!-- Security camera frame -->
  <rect x="20" y="20" width="600" height="440" fill="none" stroke="#4a5568" stroke-width="2" rx="8"/>
  
  <!-- Road scene -->
  <rect x="50" y="350" width="540" height="100" fill="#374151" opacity="0.6"/>
  
  <!-- Lane markings -->
  <line x1="50" y1="380" x2="590" y2="380" stroke="#f3f4f6" stroke-width="2" stroke-dasharray="20,10"/>
  <line x1="50" y1="420" x2="590" y2="420" stroke="#f3f4f6" stroke-width="2" stroke-dasharray="20,10"/>
  
  <!-- Cars in traffic jam -->
  <!-- Car 1 -->
  <rect x="80" y="360" width="60" height="30" fill="#dc2626" rx="6"/>
  <circle cx="95" cy="390" r="6" fill="#1f2937"/>
  <circle cx="125" cy="390" r="6" fill="#1f2937"/>
  <rect x="85" y="365" width="20" height="15" fill="#6b7280" rx="2"/>
  
  <!-- Car 2 -->
  <rect x="160" y="365" width="55" height="28" fill="#3b82f6" rx="6"/>
  <circle cx="175" cy="393" r="6" fill="#1f2937"/>
  <circle cx="200" cy="393" r="6" fill="#1f2937"/>
  <rect x="165" y="370" width="18" height="13" fill="#6b7280" rx="2"/>
  
  <!-- Car 3 -->
  <rect x="235" y="362" width="65" height="32" fill="#10b981" rx="6"/>
  <circle cx="252" cy="394" r="6" fill="#1f2937"/>
  <circle cx="283" cy="394" r="6" fill="#1f2937"/>
  <rect x="242" y="368" width="22" height="15" fill="#6b7280" rx="2"/>
  
  <!-- Car 4 -->
  <rect x="320" y="358" width="58" height="30" fill="#f59e0b" rx="6"/>
  <circle cx="335" cy="388" r="6" fill="#1f2937"/>
  <circle cx="363" cy="388" r="6" fill="#1f2937"/>
  <rect x="325" y="363" width="20" height="15" fill="#6b7280" rx="2"/>
  
  <!-- Car 5 -->
  <rect x="395" y="363" width="62" height="29" fill="#8b5cf6" rx="6"/>
  <circle cx="412" cy="392" r="6" fill="#1f2937"/>
  <circle cx="440" cy="392" r="6" fill="#1f2937"/>
  <rect x="402" y="368" width="21" height="14" fill="#6b7280" rx="2"/>
  
  <!-- Car 6 -->
  <rect x="475" y="360" width="60" height="31" fill="#ec4899" rx="6"/>
  <circle cx="490" cy="391" r="6" fill="#1f2937"/>
  <circle cx="520" cy="391" r="6" fill="#1f2937"/>
  <rect x="480" y="365" width="20" height="15" fill="#6b7280" rx="2"/>
  
  <!-- Traffic density overlay -->
  <rect x="70" y="350" width="480" height="80" fill="none" stroke="#10b981" stroke-width="3" opacity="0.7" filter="url(#greenGlow)"/>
  
  <!-- Congestion indicator -->
  <rect x="250" y="150" width="140" height="60" fill="rgba(16, 185, 129, 0.9)" rx="8" filter="url(#greenGlow)"/>
  <text x="320" y="175" fill="white" font-family="Arial" font-size="14" font-weight="bold" text-anchor="middle">
    TRAFFIC DETECTED
  </text>
  <text x="320" y="195" fill="white" font-family="Arial" font-size="12" text-anchor="middle">
    Congestion Level: HIGH
  </text>
  
  <!-- Vehicle count -->
  <rect x="480" y="250" width="120" height="40" fill="rgba(16, 185, 129, 0.8)" rx="6"/>
  <text x="540" y="270" fill="white" font-family="Arial" font-size="12" font-weight="bold" text-anchor="middle">
    Vehicles: 6
  </text>
  <text x="540" y="285" fill="white" font-family="Arial" font-size="10" text-anchor="middle">
    Speed: &lt;5 mph
  </text>
  
  <!-- Traffic flow arrows (slow/stopped) -->
  <polygon points="60,370 75,375 60,380" fill="#f59e0b" opacity="0.6"/>
  <polygon points="550,370 565,375 550,380" fill="#f59e0b" opacity="0.6"/>
  
  <!-- Traffic light simulation -->
  <rect x="580" y="100" width="15" height="40" fill="#374151" rx="2"/>
  <circle cx="587.5" cy="110" r="4" fill="#dc2626" opacity="0.8"/>
  <circle cx="587.5" cy="120" r="4" fill="#f59e0b" opacity="1"/>
  <circle cx="587.5" cy="130" r="4" fill="#6b7280" opacity="0.3"/>
  
  <!-- Camera info -->
  <rect x="20" y="430" width="200" height="30" fill="rgba(0,0,0,0.8)" rx="4"/>
  <text x="30" y="450" fill="#f3f4f6" font-family="Arial" font-size="14" font-weight="bold">
    ENTRANCE - CAM 03
  </text>
  
  <!-- Traffic status -->
  <rect x="480" y="30" width="140" height="25" fill="#10b981" rx="4" filter="url(#greenGlow)"/>
  <text x="550" y="47" fill="white" font-family="Arial" font-size="12" font-weight="bold" text-anchor="middle">
    ðŸš¦ TRAFFIC JAM
  </text>
  
  <!-- Timestamp -->
  <rect x="520" y="430" width="100" height="30" fill="rgba(0,0,0,0.8)" rx="4"/>
  <text x="570" y="450" fill="#f3f4f6" font-family="Arial" font-size="14" font-weight="bold" text-anchor="middle">
    08:25:18
  </text>
</svg>
